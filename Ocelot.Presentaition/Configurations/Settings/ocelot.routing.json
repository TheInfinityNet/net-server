{
  "Routes": [
    {
      "DownstreamPathTemplate": "/identity/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8282
        }
      ],
      "UpstreamPathTemplate": "/identity/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      /*"AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }*/
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 60,
        "Limit": 5
      },
      /*"FileCacheOptions": {
        "TtlSeconds": 15,
        "Region": "identity-cache"
      },*/
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 2,
        "DurationOfBreak": 5000,
        "TimeoutValue": 2000
      },
      "SwaggerKey": "identity"
    },
    {
      "DownstreamPathTemplate": "/profile/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 8283
        }
      ],
      "UpstreamPathTemplate": "/profile/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      /*"AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }*/
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 60,
        "Limit": 5
      },
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 2,
        "DurationOfBreak": 5000,
        "TimeoutValue": 2000
      },
      "SwaggerKey": "profile"
    }
  ]
}
